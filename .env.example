# Environment Variables Template
# Copy this file to .env.local and fill in your actual API keys

# AI Provider Selection (gemini | openai | claude)
NEXT_PUBLIC_AI_PROVIDER=gemini

# Gemini AI (Google)
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Doubao AI (豆包-字节跳动，用于国内用户)
NEXT_PUBLIC_DOUBAO_API_KEY=your_doubao_api_key_here

# OpenAI (optional, for future use)
# NEXT_PUBLIC_OPENAI_API_KEY=sk-your_openai_key_here

# Claude (optional, for future use)
# NEXT_PUBLIC_CLAUDE_API_KEY=your_claude_key_here

# Google Analytics (optional, for overseas traffic tracking)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Baidu Analytics (百度统计，用于国内访问统计)
NEXT_PUBLIC_BAIDU_ANALYTICS_ID=your_baidu_site_id_here
